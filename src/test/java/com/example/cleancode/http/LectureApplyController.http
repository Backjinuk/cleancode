### 1. 강의 신청 (POST 요청)
POST http://localhost:8080/LectureApply/applyForLecture
Content-Type: application/json

{
  "memberDto": {
    "id": 1,
    "name": "Member01"
  },
  "lectureInstanceDto": {
    "id": 1,
    "startDate": "2024-11-01",
    "endDate": "2024-12-10",
    "maxParticipants": 30,
    "currentParticipants": 10,
    "status": "OPEN",
    "lectureDto": {
      "id": 1,
      "title": "Clean Architecture",
      "instructor": "허재"
    }
  }
}

